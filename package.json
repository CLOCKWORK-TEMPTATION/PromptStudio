    "file-saver": "^2.0.5",
    "framer-motion": "^11.0.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.469.0",
    "next": "^16.0.0",
    "openai": "^4.24.0",
