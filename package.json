{
  "name": "prompt-studio",
  "version": "1.0.0",
  "description": "AI Prompt Engineering Studio with SDK Auto-Generation and Cloud Deployment",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit && tsc -p backend-tsconfig.json --noEmit",
    "backend:dev": "tsx watch src/backend/index.ts",
    "backend:build": "tsc -p backend-tsconfig.json",
    "frontend:dev": "vite",
    "frontend:build": "tsc && vite build",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
